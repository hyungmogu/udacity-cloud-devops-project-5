apiVersion: v1
kind: Service
metadata:
  name: gateway-service
  namespace: image-converter-main
  labels:
    app: gateway
spec:
  type: NodePort
  selector:
    app: gateway-main-pod
  ports:
    - protocol: "TCP"
      port: $SERVER_GATEWAY_PORT
      targetPort: $SERVER_GATEWAY_PORT
      nodePort: 30001