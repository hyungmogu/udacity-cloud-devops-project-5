apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: server
  namespace: imgconverter
spec:
  rules:
  - host: $SERVER_HOST_NAME
    http:
      paths:
      - backend:
          service:
            name: server
            port:
              number: $SERVER_POD_PORT
        path: /
        pathType: Prefix